{
  "name": "whistle.wisdom-dev",
  "version": "1.1.0",
  "description": "Ones Project 多语言多环境配置独立开发",
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist",
    "release": "standard-version --release-as patch",
    "release-minor": "standard-version --release-as minor",
    "release-major": "standard-version --release-as major",
    "build": "npm run clean & run-p build:*",
    "build:client": "webpack --mode=production --progress ",
    "build:server": "tsc  -p ./src/server/tsconfig.json",
    "dev": "run-p dev:*",
    "dev:w2": "w2 stop & lack watch & w2 run",
    "dev:server": "tsc -w -p ./src/server/tsconfig.json",
    "dev:client-pack": "webpack --mode=development --watch --progress",
    "dev:client-view": "npm run dev:client-pack & webpack serve --config webpack.serve.config  --progress",
    "prepublish": "npm run test & npm run clean & npm run build"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@smui/button": "^7.0.0-beta.0",
    "@smui/dialog": "^7.0.0-beta.0",
    "@smui/form-field": "^7.0.0-beta.0",
    "@smui/layout-grid": "^7.0.0-beta.0",
    "@smui/radio": "^7.0.0-beta.0",
    "@tsconfig/svelte": "^3.0.0",
    "@types/cookie": "^0.5.1",
    "@types/jest": "^29.2.3",
    "@types/js-cookie": "^3.0.2",
    "@types/koa": "^2.13.4",
    "@types/koa-bodyparser": "^4.3.5",
    "@types/koa-router": "^7.4.4",
    "@types/koa-static": "^4.0.2",
    "@types/url-parse": "^1.4.8",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.2",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "jest": "^29.3.1",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "scss-loader": "^0.0.1",
    "smui-theme": "^7.0.0-beta.0",
    "standard-version": "^9.5.0",
    "style-loader": "^3.3.1",
    "svelte": "^3.53.1",
    "svelte-loader": "^3.1.4",
    "svelte-preprocess": "^4.10.7",
    "ts-jest": "^29.0.3",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@colorfuldots/svelteit": "^1.0.0-alpha.7",
    "cookie": "^0.5.0",
    "js-cookie": "^3.0.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "url-parse": "^1.5.10"
  },
  "jest": {
    "testEnvironment": "node",
    "preset": "ts-jest",
    "coverageReporters": [
      "lcov",
      "json-summary"
    ]
  },
  "maintainers": [
    "ones"
  ],
  "publishConfig": {
    "registry": "https://npm2.myones.net/",
    "access": "public"
  }
}
